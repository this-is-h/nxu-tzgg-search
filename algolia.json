{
    "index_name": "nxu-tzgg-search",
    "start_urls": ["https://nxu-tzsearch.thisish.cn/pages"],
    "rateLimit": 8,
    "maxDepth": 10,
    "selectors": {
        "lvl0": {
            "selector": "",
            "defaultValue": "Documentation"
        },
        "lvl1": ".ar_article_box h5",
        "content": "ar_article p, .ar_article span",
        "lang": {
            "selector": "/html/@lang",
            "type": "xpath",
            "global": true,
            "default_value": "zh-CN"
        }
    },
    "custom_settings": {
        "attributesForFaceting": ["lang"]
    }
}